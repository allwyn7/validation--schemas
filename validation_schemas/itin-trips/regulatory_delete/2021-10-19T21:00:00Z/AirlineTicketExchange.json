{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "AirlineTicketExchange.json",
	"type": "object",
	"properties": {
		"PlatingCarrierNumericCode": {
			"type": "string",
			"maxLength": 3,
			"group": "external",
			"classification": "personal",
