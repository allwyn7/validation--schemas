		"SemanticsCode": {
			"type": "string",
			"maxLength": 15,
			"group": "external",
			"classification": "none",
			"description": ""
		},
		"SemanticsVendorType": {
			"type": "string",
			"maxLength": 1,
			"pattern": "[XCHAEFGPR]",
			"group": "external",
			"classification": "none",
			"description": ""
		}
	},
	"additionalProperties": false
}