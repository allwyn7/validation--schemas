{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "CustomAttribute.json",
	"type": "object",
	"properties": {
		"ExternalId": {
			"type": "integer",
			"group": "external"
		},
		"DataType": {
			"type": "string",
			"group": "external"
		},
		"Name": {
			"type": "string",
			"maxLength": 128,
			"group": "external"
		},
		"DisplayTitle": {
			"type": "string",
			"maxLength": 128,
			"group": "external"
		},
		"Data": {
			"type": "string",
			"maxLength": 255,
			"group": "external"
		},
		"DisplayValue": {
			"type": "string",
			"maxLength": 255
		},
		"DisplayOnItinerary": {
			"type": "boolean",
			"group": "external"
		}
	},
	"additionalProperties": false
}
